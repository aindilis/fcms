body {
  padding: 0;
  margin: 0;
}

iframe.two_column {
  position: absolute;
  _position: static; /* IE <= 6 */
  border: 0;
  top: 0;
  width: 50%;
  height: 100%;
}

iframe#old_iframe {
  left: 0;
}

iframe#new_iframe {
  right: 0;
}

.clearer {
  clear: both;
}
